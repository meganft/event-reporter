<html>
<head>
<style>
  table, th, td {
        border: 1px solid black;
  }
</style>

<title>Event Reporter Data Report</title>
</head>
<body bgcolor = "#DCDCDC">

<h1><font color="blue"><center>Event Attendee Information</center></font></h1>

  <table>
  <tr>
    <th width="10%", align="left">Last Name</th>
    <th width="10%", align="left">First Name</th>
    <th width="20%", align="left">Email</th>
    <th width="5%", align="left">Zipcode</th>
    <th width="15%">City</th>
    <th width="10%", align="left">State</th>
    <th width="23%", align="left">Address</th>
    <th width="8%", aligh="left">Homephone</th>
    <th width="5%", align="left">District</th>
  </tr>

  <tr>

    <% queue.each do |attendee| %>
      <td><%= "#{attendee["last_name"]}" %></td>
      <td><%= "#{attendee["first_name"]}" %></td>
      <td><%= "#{attendee["email_address"]}" %></td>
      <td><%= "#{attendee["zipcode"]}" %></td>
      <td><%= "#{attendee["city"]}" %></td>
      <td><%= "#{attendee["state"]}" %></td>
      <td><%= "#{attendee["street"]}" %></td>
      <td><%= "#{attendee["homephone"]}" %></td>
      <td><%= "#{attendee["district"]}" %></td>
    </tr>
      <% end %>
  </table>
</body>
</html>
